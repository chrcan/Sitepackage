{
	"name": "rcdesign/rcdesign",
	"type": "typo3-cms-extension",
	"description": "Sitepackage for TYPO3 12",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/minimal": "^12.0",
		"georgringer/news": "^11.4",
		"b13/container": "^2.3",
		"contentblocks/content-blocks": "^0.7"
	},
	"authors": [
		{
			"name": "Christian Racan",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3 CMS",
		"Rcdesign"
	],
	"support": {
		"issues": "https://github.com/vendorname/my-extensions/issues"
	},
	"funding": [
		{
			"type": "other",
			"url:": "myfundpage.org/vendorname"
		}
	],
	"homepage": "https://typo3.org",
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"typo3/cms-belog": "^12.0",
		"typo3/cms-lowlevel": "^12.0",
		"typo3/cms-scheduler": "^12.0",
		"typo3/cms-setup": "^12.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "rcdesign"
		}
	},
	"autoload": {
		"psr-4": {
			"Rcdesign\\Rcdesign\\": "Classes"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}