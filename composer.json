{
	"name": "rcdesign/sitepackage",
	"type": "typo3-cms-extension",
	"description": "Sitepackage for TYPO3 12",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/minimal": "^13.0"
	},
	"authors": [
		{
			"name": "Christian Racan",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3 CMS",
		"Rcdesign"
	],
	"homepage": "https://typo3.org",
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"typo3/cms-belog": "^13.0",
		"typo3/cms-lowlevel": "^13.0",
		"typo3/cms-scheduler": "^13.0",
		"typo3/cms-setup": "^13.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "rcdesign"
		}
	},
	"autoload": {
		"psr-4": {
			"Rcdesign\\Rcdesign\\": "Classes"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}